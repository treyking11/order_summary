<div class="form-group">
  <% @creative_types.each do |creative_name| %>
    <div class="form-check">
      <label class="form-check-label">
        <%= f.check_box(:creative_types, {:multiple => true}, creative_name, nil) %>
        <%= creative_name %>
      </label>
    </div>
  <% end %>
</div>
